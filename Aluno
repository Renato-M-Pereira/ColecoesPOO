//Crie uma classe Aluno com campos nome e nota.
//Em seguida, crie um ArrayList de alunos e,
//usando Comparator, ordene-os primeiro por nome e depois por nota.

public class Aluno {
    private String nome;
    private double nota;


    public Aluno(String nome, double nota) {

        this.nome = nome;

        this.nota = nota;
    }


    public String getNome() {
        return nome;
    }

    public double getNota() {
        return nota;
    }

    @Override
    public String toString() {
        return "Aluno{" + "nome = '" + nome + '\'' + ", " +
                "nota = " + nota + '}';
    }
}
